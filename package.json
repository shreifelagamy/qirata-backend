{
    "name": "express-backend",
    "version": "1.0.0",
    "description": "Express.js TypeScript backend with TypeORM and Socket.io",
    "main": "dist/app.js",
    "scripts": {
        "start": "node dist/app.js",
        "start:original": "node dist/app-original.js",
        "dev": "nodemon --exec ts-node src/app.ts",
        "dev:original": "nodemon --exec ts-node src/app-original.ts",
        "dev:ai": "chmod +x ./scripts/start-with-ai.sh && ./scripts/start-with-ai.sh",
        "build": "tsc",
        "typeorm": "typeorm-ts-node-commonjs",
        "migration:generate": "npm run typeorm migration:generate",
        "migration:run": "npm run typeorm migration:run -- -d src/app.ts",
        "migration:revert": "npm run typeorm migration:revert",
        "ai:health-check": "chmod +x ./scripts/ai-health-check.sh && ./scripts/ai-health-check.sh",
        "ai:test": "chmod +x ./scripts/run-ai-tests.sh && ./scripts/run-ai-tests.sh",
        "test:ai": "npm test src/services/ai/ && npm test src/tests/ai-*.test.js",
        "ollama:check": "curl -s http://localhost:11434/api/version || echo 'Ollama not running'",
        "setup:full": "npm install && npm run migration:run && npm run ai:health-check",
        "setup:scripts": "chmod +x ./scripts/*.sh"
    },
    "keywords": [
        "express",
        "typescript",
        "typeorm",
        "socket.io"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@langchain/community": "^0.3.45",
        "@langchain/langgraph": "^0.3.5",
        "@langchain/ollama": "^0.2.1",
        "@langchain/openai": "^0.5.11",
        "@types/nodemailer": "^7.0.0",
        "@types/turndown": "^5.0.5",
        "axios": "^1.9.0",
        "bcryptjs": "^2.4.3",
        "better-auth": "^1.3.7",
        "cheerio": "^1.0.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-rate-limit": "^7.5.0",
        "express-validator": "^7.0.1",
        "feedparser": "^2.2.10",
        "helmet": "^7.0.0",
        "jose": "^6.0.12",
        "jsonwebtoken": "^9.0.0",
        "langchain": "^0.3.27",
        "morgan": "^1.10.0",
        "nodemailer": "^7.0.5",
        "pg": "^8.11.0",
        "rate-limiter-flexible": "^7.1.1",
        "sanitize-html": "^2.16.0",
        "socket.io": "^4.6.1",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "turndown": "^7.2.0",
        "typeorm": "^0.3.16",
        "winston": "^3.8.2",
        "zod": "^3.25.42"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/express-rate-limit": "^5.1.3",
        "@types/feedparser": "^2.2.8",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/morgan": "^1.9.4",
        "@types/node": "^20.2.1",
        "@types/pg": "^8.6.6",
        "@types/sanitize-html": "^2.16.0",
        "@types/socket.io": "^3.0.2",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.8",
        "nodemon": "^3.1.10",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4"
    }
}
